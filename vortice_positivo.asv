[x,y] = meshgrid(-5:0.1:5,-5:0.1:5);
%r = sqrt((x^2)+(y^2));
%theta = atan2(y./r,x./r);
b = 4;
Gamma = 2;
z       = x + 1i*y;
theta   = angle(z);
r       = abs(z);
U = 2;
W = (Gamma/(2*pi))*ln;
phi = real(W);
psi = imag(W);
V = conj(W);
v=abs(V);

figure
contour(x,y,phi,25);
figure
contour(x,y,psi,25)
figure
contour(x,y,v,25)